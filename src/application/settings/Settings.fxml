<AnchorPane fx:id="rootPane" prefHeight="308.0" prefWidth="514.0" stylesheets="@../css/movierental.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.settings.SettingsController">
   <children>
