                     <font>
                        <Font size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="maxRented" layoutX="170.0" layoutY="18.0" prefHeight="32.0" prefWidth="175.0" />
               </children>
            </Pane>
            <Pane layoutX="10.0" layoutY="110.0" prefHeight="48.0" prefWidth="449.0">
               <children>
