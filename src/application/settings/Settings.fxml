                        <ImageView fitHeight="30.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../Icons/left-arrow.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
            </Pane>
            <Pane prefHeight="48.0" prefWidth="449.0">
               <children>
                  <Label layoutX="71.0" layoutY="25.0" text="Set movie price" textFill="WHITE">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Label>
                  <TextField fx:id="cost" layoutX="170.0" layoutY="19.0" prefHeight="32.0" prefWidth="175.0" />
               </children>
            </Pane>
            <Pane layoutX="10.0" layoutY="62.0" prefHeight="48.0" prefWidth="449.0">
